<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="609.0" prefWidth="473.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.CitiLoginController">
   <children>
      <ImageView fitHeight="394.0" fitWidth="491.0" layoutY="-1.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../자바프로젝트/images/images.png" />
         </image>
      </ImageView>
      <HBox alignment="CENTER" layoutX="-2.0" layoutY="538.0" prefHeight="51.0" prefWidth="491.0" spacing="50.0">
         <children>
            <Button fx:id="citiBtnjoin" mnemonicParsing="false" text="회원가입" />
            <Button fx:id="citibtnLogin" mnemonicParsing="false" text="로그인" />
            <Button fx:id="citibtnClose" mnemonicParsing="false" text="닫기" />
         </children>
      </HBox>
      <VBox alignment="CENTER" layoutX="228.0" layoutY="398.0" prefHeight="137.0" prefWidth="194.0" spacing="10.0">
         <children>
            <TextField fx:id="cititextId1" prefHeight="39.0" prefWidth="82.0" promptText="호 수" />
            <TextField fx:id="cititextId2" prefHeight="33.0" prefWidth="82.0" promptText="동 수" />
            <PasswordField fx:id="cititextPassword" promptText="비밀번호입력" />
         </children>
      </VBox>
      <VBox layoutX="121.0" layoutY="400.0" prefHeight="123.0" prefWidth="116.0" spacing="80.0">
         <children>
            <Label text="세대 주소">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Label>
            <Label text="비밀 번호" />
         </children>
      </VBox>
   </children>
</AnchorPane>
